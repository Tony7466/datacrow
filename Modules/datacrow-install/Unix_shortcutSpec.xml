<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

  <programGroup defaultName="Data Crow" location="applications"/>

  <skipIfNotSupported/>

  <shortcut
     name="DataCrow"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="yes"
